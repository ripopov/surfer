{
  "configurations": [
    {
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "name": "Run Extension",
      "outFiles": [
        "${workspaceFolder}/out/*.js"
      ],
      "preLaunchTask": "${defaultBuildTask}",
      "request": "launch",
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "type": "extensionHost"
    }
  ],
  "version": "0.2.0"
}
